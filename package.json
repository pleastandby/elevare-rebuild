{
  "name": "elevare-monorepo",
  "version": "1.0.0",
  "description": "Elevare application with frontend and backend",
  "scripts": {
    "dev:frontend": "cd elevare && npm run dev",
    "dev:backend": "cd backend && npm run server",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "install:all": "cd backend && npm install && cd ../elevare && npm install",
    "build:frontend": "cd elevare && npm run build",
    "start:backend": "cd backend && npm start"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "workspaces": [
    "backend",
    "elevare"
  ]
}
